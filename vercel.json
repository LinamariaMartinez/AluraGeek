{
  "version": 2,
  "name": "alurageek",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/productos",
      "dest": "/api/productos.js"
    },
    {
      "src": "/api/productos/(.*)",
      "dest": "/api/productos.js"
    },
    {
      "src": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/productos.js": {
      "maxDuration": 10
    }
  }
}
